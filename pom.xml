        <developerConnection>scm:git:git://github.com/AssemblyAI/assemblyai-java-sdk.git</developerConnection>
        <url>https://github.com/AssemblyAI/assemblyai-java-sdk</url>
    </scm>
    <dependencies>
        <dependency>
            <groupId>com.assemblyai</groupId>
            <artifactId>assemblyai-java</artifactId>
            <version>2.3.1</version>
        </dependency>
        <dependency>
